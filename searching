//LINEAR SEARCH
#include <bits/stdc++.h>
using namespace std;
int linear_search(int a[], int n, int key)
{
    for (int i = 0; i < n; i++)
    {
        if (key == a[i])
        {
            return i;
        }
    }
    return -1;
}

int main()
{
    int a[] = {1, 6, 4, 3, 2, 10, 88};
    int n = sizeof(a) / sizeof(a[0]);
    int key;
    cin >> key;
    int index = linear_search(a, n, key);
    if (index == -1)
    {
        cout << key << " isn't present";
    }
    else
    {
        cout << key << " is present at index " << index;
    }

    return 0;
}

//BINARY SEARCH

